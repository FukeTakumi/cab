npx sequelize-cli model:generate --name come_message --attributes name:string,island_name:string,fruit:string,cab_status:string,cab_bell:integer,want:string,pass:string --underscored
npx sequelize-cli model:generate --name go_message --attributes name:string,island_name:string,memo:string --underscored


'use strict';
module.exports = (sequelize, DataTypes) => {
  const come_message = sequelize.define('come_message', {
    name: DataTypes.STRING,
    island_name: DataTypes.STRING,
    fruit: DataTypes.STRING,
    cab_status: DataTypes.STRING,
    cab_bell: DataTypes.INTEGER,
    want: DataTypes.STRING,
    pass: DataTypes.STRING
  }, {
    underscored: true,
  });
  come_message.associate = function(models) {
    come_message.hasMany(models.go_message);
  };
  return come_message;
};


'use strict';
module.exports = (sequelize, DataTypes) => {
  const go_message = sequelize.define('go_message', {
    name: DataTypes.STRING,
    island_name: DataTypes.STRING,
    memo: DataTypes.STRING,
    goto_id: DataTypes.STRING
  }, {
    underscored: true,
  });
  go_message.associate = function(models) {
    go_message.belongsTo(models.come_message);
  };
  return go_message;
};